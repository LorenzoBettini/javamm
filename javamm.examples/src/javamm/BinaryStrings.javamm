void printArray(int[] a) {
  int na = a.length;
  for (int i = 0; i < na; i++) {
    System.out.print(a[i] + " ");
  }
  System.out.println();
}
void generate( int[] a, int b ) {
  if (b==0) {
    printArray( a );
  } else {
    a[b-1] = 0;
    generate( a, b-1 );
    a[b-1] = 1;
    generate( a, b-1 );
  }
}

int[] a = {0,0,0,0};
generate(a,4);
